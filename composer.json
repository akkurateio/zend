{
    "name": "subvitaminetm/zend",
    "description": "A package that allows you to send emails (Mailjet) , SMS (SmsFactor) and manage PDF pagination from Zend 1 project.",
    "type": "library",
    "version": "1.1.6",
    "authors": [
        {
            "name": "Subvitamine",
            "email": "hello@subvitamine.com"
        }
    ],
    "require": {
        "mailjet/mailjet-apiv3-php": "1.3.0",
        "curl/curl": "^1.5",
        "voku/html2text": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Subvitamine\\Mailjet\\": "src/Mailjet",
            "Subvitamine\\Smsfactor\\": "src/Smsfactor",
            "Subvitamine\\Pdf\\": "src/Pdf"
        }
    }
}
